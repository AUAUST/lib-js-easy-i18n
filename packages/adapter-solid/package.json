{
  "name": "@auaust/easy-i18n-adapter-solid",
  "version": "2.0.0",
  "description": "The adapter that makes easy-i18n work with Solid smoothly.",
  "license": "GPL-3.0-only",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm build:clean && pnpm build:js",
    "build:clean": "rimraf dist",
    "build:js": "tsup",
    "dev": "pnpm build:clean && pnpm dev:js",
    "dev:js": "tsup --watch",
    "test": "echo \"No test implemented yet.\"",
    "coverage": "pnpm vitest --coverage",
    "typecheck": "echo 'Typechecking...' && tsc --noEmit && echo 'All good!'",
    "ci:pack": "pnpm pack"
  },
  "peerDependencies": {
    "@auaust/easy-i18n": "workspace:^",
    "solid-js": "^1.8.8"
  },
  "devDependencies": {
    "@auaust/easy-i18n": "workspace:^",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "esbuild-plugin-solid": "^0.6.0",
    "esbuild": "^0.24.0",
    "jsdom": "^25.0.1",
    "rimraf": "^6.0.1",
    "solid-js": "^1.9.3",
    "tsup-preset-solid": "^2.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite-plugin-solid": "^2.11.0",
    "vite": "^6.0.3",
    "vitest": "^1.1.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "pnpm@9.15.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/dev.cjs"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "browser": {},
  "typesVersions": {}
}
