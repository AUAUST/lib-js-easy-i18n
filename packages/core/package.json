{
  "name": "@auaust/easy-i18n",
  "version": "0.1.12",
  "description": "The only i18n library you'll ever need. Easy, flexible, lightweight, powerful, type-safe.",
  "license": "GPL-3.0-only",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm test && rollup -c",
    "test": "vitest run --config vitest.config.ts --",
    "watch": "vitest --config vitest.config.ts --",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "source": "src/index.ts",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.23.8",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/babel__core": "^7.20.5",
    "@types/babel__plugin-transform-runtime": "^7.9.5",
    "@types/babel__preset-env": "^7.9.6",
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.1.3",
    "rollup": "^4.9.5",
    "rollup-plugin-ts": "^3.4.5",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@auaust/primitive-kit": "^0.9.4"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  }
}
